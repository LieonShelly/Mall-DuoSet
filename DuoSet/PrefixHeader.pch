/**
 *  @prama 屏幕大小
 *  mainScreenWidth   屏幕宽度
 *  mainScreenHeight  屏幕高度
 */
#define mainScreenWidth   [UIScreen mainScreen].bounds.size.width
#define mainScreenHeight  [UIScreen mainScreen].bounds.size.height
#define refreshScreen CGRectMake(0, 64, mainScreenWidth, mainScreenHeight - 64);

/**
 *  @prama 适配屏幕标准
 */
//# define ORIGIN_HEIGHT  568.f
//# define ORIGIN_WIDTH   320.f
# define ORIGIN_HEIGHT  667.f
# define ORIGIN_WIDTH   375.f
# define DH_INLINE   static inline
# define SCREEN_SIZE [UIScreen mainScreen].bounds.size
DH_INLINE CGFloat AdapterHeight()
{
    return SCREEN_SIZE.height / ORIGIN_HEIGHT;
}

DH_INLINE CGFloat AdapterWidth()
{
    return SCREEN_SIZE.width / ORIGIN_WIDTH;
}

DH_INLINE CGPoint DHFlexibleCenter(CGPoint center)
{
    return CGPointMake(center.x * AdapterWidth(), center.y * AdapterHeight());
}
#define ADAPT_IPHONE4

#ifdef ADAPT_IPHONE4
DH_INLINE CGSize DHFlexibleSize(CGSize size, BOOL adjustWidth)
{
    if (adjustWidth) {
        return CGSizeMake(size.width * AdapterHeight(), size.height*AdapterHeight());
    }
    
    return CGSizeMake(size.width * AdapterWidth(), size.height * AdapterHeight());
}

DH_INLINE CGRect DHFlexibleFrame(CGRect frame, BOOL adjustWidth)
{
    CGPoint center = CGPointMake(frame.origin.x + frame.size.width/2, frame.origin.y + frame.size.height/2);
    
    center = DHFlexibleCenter(center);
    
    CGRect retFrame ;
    CGSize size = DHFlexibleSize(frame.size, adjustWidth);
    retFrame.origin.x = center.x - size.width/2;
    retFrame.origin.y = center.y - size.height/2;
    retFrame.size = size;
    return retFrame;
}

#else
DH_INLINE CGSize DHFlexibleSize(CGSize size)
{
    return CGSizeMake(size.width * AdapterWidth(), size.height * AdapterHeight());
    
}

DH_INLINE CGRect DHFlexibleFrame(CGRect frame)
{
    CGFloat x = frame.origin.x * AdapterWidth();
    CGFloat y = frame.origin.y * AdapterHeight();
    
    CGRect retFrame ;
    retFrame.origin.x = x;
    retFrame.origin.y = y;
    CGSize size = DHFlexibleSize(frame.size);
    retFrame.size = size;
    return retFrame;
}
#endif

